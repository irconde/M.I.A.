{
    "name": "dna-client-ui",
    "productName": "Pilot GUI",
    "main": "public/electron.js",
    "version": "3.6.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "@babel/core": "^7.13.0",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@material-ui/core": "^4.12.2",
        "@material-ui/icons": "^4.11.2",
        "@mui/material": "^5.0.0",
        "@reduxjs/toolkit": "^1.5.0",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.6.3",
        "babel-preset-react": "^6.24.1",
        "browser-fs-access": "^0.20.5",
        "concurrently": "^6.4.0",
        "cornerstone-core": "^2.3.0",
        "cornerstone-math": "^0.1.9",
        "cornerstone-wado-image-loader": "^3.0.6",
        "cornerstone-web-image-loader": "^2.1.1",
        "dcmjs": "^0.17.1",
        "dicom-parser": "1.8.7",
        "dotenv": "^8.2.0",
        "eac-cornerstone-tools": "^1.0.6",
        "electron-is-dev": "^2.0.0",
        "hammerjs": "^2.0.8",
        "jszip": "^3.5.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.debounce": "4.0.8",
        "npm-run-all": "^4.1.5",
        "randomcolor": "^0.6.2",
        "react": "^16.13.1",
        "react-color": "^2.19.3",
        "react-cookie": "^4.0.3",
        "react-cornerstone-viewport": "^4.0.4",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.2",
        "react-scripts": "^4.0.1",
        "socket.io-client": "^2.3.0",
        "styled-components": "^5.2.1",
        "uuid": "^8.3.2"
    },
    "resolutions": {
        "react-dev-utils": "^10.2.1"
    },
    "scripts": {
        "start": "npm-run-all -p load-env react",
        "start-electron": "run-script-os",
        "start-electron:win32": "set BROWSER=none && concurrently \"npm run start\" \"npm run load-electron\"",
        "start-electron:darwin:linux": "export BROWSER=none && concurrently \"npm run start\" \"npm run load-electron\"",
        "load-electron": "electron .",
        "load-env": "node -r dotenv/config",
        "react": "react-scripts start",
        "build": "react-scripts build",
        "build-electron": "npm run build && npm run package-electron",
        "package-electron": "electron-builder",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
    },
    "build": {
        "appId": "edu.ualr.eac.dna.pilot.gui"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
        "babel-eslint": "^10.1.0",
        "electron": "^15.3.1",
        "electron-builder": "^22.13.1",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "jsdoc": "^3.6.7",
        "prettier": "^2.2.1",
        "run-script-os": "^1.1.6"
    }
}
